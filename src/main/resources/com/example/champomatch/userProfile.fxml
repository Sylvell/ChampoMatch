<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" style="-fx-background-color: white;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.champomatch.userProfileController">
   <children>
      <Button fx:id="returned" alignment="TOP_LEFT" layoutY="1.0" mnemonicParsing="false" onAction="#goback" style="-fx-background-color: transparent; -fx-border-color: transparent;" text="&lt;- Return" />
      <CheckBox fx:id="edit" layoutX="91.0" layoutY="1.0" mnemonicParsing="false" onAction="#editUser" prefHeight="18.0" prefWidth="94.0" styleClass="edit" text="Edit Profile" textFill="WHITE" />
      <GridPane layoutX="25.0" layoutY="63.0" prefHeight="323.0" prefWidth="750.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="388.0" minWidth="10.0" prefWidth="105.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="681.0" minWidth="10.0" prefWidth="681.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label style="-fx-background-color: transparent;" text="Name : ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      
            <TextField fx:id="name" editable="false" focusTraversable="false" mouseTransparent="true" promptText="Name" style="-fx-background-color: transparent; -fx-text-color: black;" GridPane.columnIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Label style="-fx-background-color: transparent;" text="Age : " GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="email" editable="false" focusTraversable="false" mouseTransparent="true" promptText="Age" style="-fx-background-color: transparent; -fx-text-color: black;" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Label style="-fx-background-color: transparent;" text="Admin :" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="admin" editable="false" focusTraversable="false" mouseTransparent="true" promptText="Status" style="-fx-background-color: transparent; -fx-text-color: black;" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
         </children>
      </GridPane>
   </children>
</AnchorPane>
